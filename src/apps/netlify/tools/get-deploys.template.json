{
  "slug": "get-deploys",
  "name": "Get Deploys",
  "description": "Get recent deploys from Netlify.",
  "type": "tool",
  "file": "get-deploys.ts",
  "app": "netlify",
  "inputSchema": [
    { "displayName": "siteName", "type": "string", "required": false, "description": "Site name to filter" },
