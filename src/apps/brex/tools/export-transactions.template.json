{
  "slug": "export-transactions",
  "name": "Export Transactions",
  "description": "Export transactions from Brex for a date range.",
  "type": "tool",
  "file": "export-transactions.ts",
  "app": "brex",
  "inputSchema": [
    { "displayName": "startDate", "type": "date", "required": true, "description": "Start date" },
    { "displayName": "endDate", "type": "date", "required": true, "description": "End date" },
    { "displayName": "cardId", "type": "string", "required": false, "description": "Specific card ID (optional)" }
  ],
  "outputSchema": [
    { "displayName": "transactionCount", "type": "number", "required": true, "description": "Number of transactions" },
    { "displayName": "totalSpend", "type": "number", "required": true, "description": "Total spend amount" },
    { "displayName": "success", "type": "boolean", "required": true, "description": "Whether export succeeded" }
  ]
}
