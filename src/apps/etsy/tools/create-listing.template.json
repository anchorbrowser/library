{
  "slug": "create-listing",
  "name": "Create Listing",
  "description": "Create a new product listing on Etsy.",
  "type": "tool",
  "file": "create-listing.ts",
  "app": "etsy",
  "inputSchema": [
    { "displayName": "title", "type": "string", "required": true, "description": "Listing title" },
    { "displayName": "description", "type": "string", "required": true, "description": "Listing description" },
    { "displayName": "price", "type": "number", "required": true, "description": "Price" },
    { "displayName": "quantity", "type": "number", "required": false, "description": "Quantity available", "defaultValue": "1" },
    { "displayName": "tags", "type": "string", "required": false, "description": "Comma-separated tags" }
  ],
  "outputSchema": [
    { "displayName": "listingId", "type": "string", "required": true, "description": "Created listing ID" },
    { "displayName": "success", "type": "boolean", "required": true, "description": "Whether listing was created" }
  ]
}
